<Grid x:Class="ChroZenService.met_oven"
                x:Name="uc"
             xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
              
              
             xmlns:loc="clr-namespace:ChroZenService"
             xmlns:cvt="clr-namespace:ChroZenService"
             
             HeightRequest="465" WidthRequest="980">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Grid.Resources>
            <Style TargetType="Label" BasedOn="{StaticResource HTX}">
                <Setter Property="FontSize" Value="14" />
            </Style>
        </Grid.Resources>

        <Frame  >
            <StackLayout>
                <Frame Style="{StaticResource MetGroupBK}">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition  />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition  />
                        </Grid.RowDefinitions>

                        <Frame Style="{StaticResource LineRT}" >
                            <Label Text="Max Temp" HorizontalOptions="End" />
                        </Frame>

                        <Frame Grid.Column="1" >
                            <Editor   Text="{Binding Path=MM.MaxTemp, Source=uc}" Style="{StaticResource MetTB}"  HeightRequest="20" VerticalOptions="Start" />
                        </Frame>

                        <Frame Grid.Row="1" Style="{StaticResource LineRT}" >
                            <Label Text="Equilibrium Time" HorizontalOptions="End" />
                        </Frame>

                        <Frame Grid.Row="1" Grid.Column="1" Padding="15, 1, 0, 1">
                            <Editor Text="{Binding Path=MM.EquibTime, Source=uc}" Style="{StaticResource MetTB}" />
                        </Frame>

                        <Frame Grid.Row="2" Style="{StaticResource LineRT}" Padding="0, 0, 15, 0">
                            <Label Text="Cryogenic Cooling"   HorizontalOptions="End" />
                        </Frame>

                        <Frame Grid.Row="2" Grid.Column="1" Padding="15, 1">
                            <Switch IsToggled="{Binding Path=MM.Cryogenic, Source=uc}" Style="{StaticResource MetOnOffBtn}" HorizontalOptions="Start"  />
                        </Frame>

                    </Grid>
                </Frame>

                <Frame HeightRequest="5" />

                <Frame Style="{StaticResource MetGroupBK}">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <Frame Style="{StaticResource LineRT}" Padding="0, 0, 15, 0">
                            <Label Text="Fast Cooling"   HorizontalOptions="End" />
                        </Frame>
                        <Frame Grid.Column="1" Padding="15, 3, 0, 3">
                            <Switch IsToggled="{Binding Path=MM.FastCryo, Source=uc}" Style="{StaticResource MetOnOffBtn}" HorizontalOptions="Start" />
                        </Frame>

                        <Frame Grid.Row="1" Style="{StaticResource LineRT}" Padding="0, 0, 15, 0">
                            <Label Text="Auto Readyrun"   HorizontalOptions="End" />
                        </Frame>
                        <Frame Grid.Row="1" Grid.Column="1" Padding="15, 3, 0, 3">
                            <Switch IsToggled="{Binding Path=MM.AutoReadyrun, Source=uc}" Style="{StaticResource MetOnOffBtn}" HorizontalOptions="Start" />
                        </Frame>

                    </Grid>
                </Frame>

                <Frame HeightRequest="5" />
                
                <!--// Run Start-->
                <Frame Grid.Row="5" Style="{StaticResource MetGroupBK}">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>

                        <Frame Style="{StaticResource LineRT}" Padding="0, 0, 10, 0">
                            <Label Text="Run Start"  TextColor="#ffffff"  HorizontalOptions="Start" />
                        </Frame>

                        <Frame Grid.Column="1" Padding="15, 1">
                            <Switch x:Name="btn_runstart" Style="{StaticResource MetOnOffBtn}" HorizontalOptions="Start"
                                        IsToggled="{Binding Path=MM.RSF.Onoff, Source=uc}" />
                        </Frame>

                        <Frame Grid.Row="1" Style="{StaticResource LineRT}" Padding="0, 0, 15, 0">
                            <Label Text="No. of run"   HorizontalOptions="End" />
                        </Frame>

                        <Frame Grid.Row="1" Grid.Column="1" Padding="15, 1, 0, 1">
                            <Editor Style="{StaticResource MetTB}"  
                                            IsEnabled="{Binding Source=btn_runstart, Path=IsToggled}"
                                        Text="{Binding Path=MM.RSF.Count, Source=uc}" />
                        </Frame>

                        <Frame Grid.Row="2" Style="{StaticResource LineRT}" Padding="0, 0, 15, 0">
                            <Label Text="Cycle Time"   HorizontalOptions="End" />
                        </Frame>

                        <Frame Grid.Row="2" Grid.Column="1" Padding="15, 1, 0, 1">
                            <Editor Style="{StaticResource MetTB}"   IsEnabled="{Binding Source=btn_runstart, Path=IsToggled}"
                                        Text="{Binding Path=MM.RSF.Cycletime, Source=uc}" />
                        </Frame>

                    </Grid>
                </Frame>

                <Frame HeightRequest="5" />
                <!--Post Run-->
                <Frame Style="{StaticResource MetGroupBK}">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*" />
                            <ColumnDefinition Width="1*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1*" />
                            <RowDefinition Height="1*" />
                            <RowDefinition Height="1*" />
                        </Grid.RowDefinitions>

                        <Frame Style="{StaticResource LineRT}" Padding="0, 0, 15, 0">
                            <Label Text="Post Run"  TextColor="#ffffff"  HorizontalOptions="Start" />
                        </Frame>

                        <Frame Grid.Column="1" Padding="15, 3, 0, 3">
                            <Switch x:Name="btn_postrun" Style="{StaticResource MetOnOffBtn}" HorizontalOptions="Start" 
                                        IsToggled="{Binding Path=MM.PRF.Onoff, Source=uc}" />
                        </Frame>

                        <Frame Grid.Row="1" Style="{StaticResource LineRT}" Padding="0, 0, 15, 0">
                            <Label Text="Postrun Temp"   HorizontalOptions="End" />
                        </Frame>

                        <Frame Grid.Row="1" Grid.Column="1" Padding="15, 1, 0, 1">
                            <Editor  IsEnabled="{Binding Source=btn_postrun, Path=IsToggled}" Style="{StaticResource MetTB}"  
                                        Text="{Binding Path=MM.PRF.Temp, Source=uc}" />
                        </Frame>

                        <Frame Grid.Row="2" Style="{StaticResource LineRT}" Padding="0, 0, 15, 0">
                            <Label Text="Postrun Time"   HorizontalOptions="End" />
                        </Frame>

                        <Frame Grid.Row="2" Grid.Column="1" Padding="15, 1, 0, 1">
                            <Editor IsEnabled="{Binding Source=btn_postrun, Path=IsToggled}" Style="{StaticResource MetTB}"  
                                        Text="{Binding Path=MM.PRF.Time, Source=uc}" />
                        </Frame>
                    </Grid>
                </Frame>

            </StackLayout>
        </Frame>

        <Frame Grid.Column="1" Padding="5, 5, 0, 5">
            <StackLayout>
                <Frame Style="{StaticResource MetGroupBK}">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="40" />
                            <ColumnDefinition />
                            <ColumnDefinition />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>

                        <Frame Grid.Row="0" Grid.ColumnSpan="4" Style="{StaticResource LineBT}" />
                        <Frame Grid.Column="1" Padding="5">
                            <Label Text="On/Off"  HorizontalOptions="Center"  />
                        </Frame>
                        <Frame Grid.Column="2">
                            <Label Text="Setting"  HorizontalOptions="Center"  />
                        </Frame>
                        <Frame Grid.Column="3">
                            <Label Text="Time"  HorizontalOptions="Center"  />
                        </Frame>

                        <Frame Grid.RowSpan="2" Grid.Column="0" Style="{StaticResource LineRT}" />

                        <Label Grid.Row="2" Text="Temp"  HorizontalOptions="Center"  />

                        <Frame Grid.Column="1" Grid.Row="1" Padding="5, 5, 5, 0">
                            <Switch x:Name="btn_temp" IsToggled="{Binding Path=MM.TempOnoff, Source=uc}" Style="{StaticResource MetOnOffBtn}" />
                        </Frame>

                        <Frame Grid.Column="2" Grid.Row="1" Padding="5, 5, 5, 0">
                            <Editor IsEnabled="{Binding Source=btn_temp, Path=IsToggled}" x:Name="label_temp_set" Style="{StaticResource MetTB}"  
                                           Text="{Binding Path=MM.TempSet, Source=uc}"  />
                        </Frame>
                        <!--//-->
                        <Frame Grid.Column="3" Grid.Row="1" Padding="5, 5, 5, 0">
                            <Editor IsEnabled="{Binding Source=btn_temp, Path=IsToggled}"  x:Name="label_temp_time" Style="{StaticResource MetTB}" 
                                            Text="{Binding Path=MM.InitTime, Source=uc}"
                                             />
                        </Frame>

                    </Grid>
                </Frame>

                <Frame HeightRequest="5" />

                <Frame Style="{StaticResource MetGroupBK}">
